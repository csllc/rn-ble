    "eventemitter3": "^5.0.1",
    "react-native-ble-manager": "^10.1.5"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "typescript": "^5.3.2"
  }
}
